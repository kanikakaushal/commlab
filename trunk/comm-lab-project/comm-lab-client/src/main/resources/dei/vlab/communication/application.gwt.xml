<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='application'>
   <!-- Inherit the core Web Toolkit stuff. -->                      
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.core.Core"/>
     <inherits name="com.google.gwt.http.HTTP"/>
    <inherits name="com.google.gwt.user.History"/>
    <inherits name="com.smartgwt.SmartGwtNoScript"/>
    <inherits name="com.smartgwt.tools.SmartGwtTools"/>
 
    <inherits name="com.smartclient.theme.enterprise.EnterpriseResources"/>
    <inherits name="com.smartclient.theme.enterpriseblue.EnterpriseBlueResources"/>
    <inherits name="com.smartclient.theme.graphite.GraphiteResources"/>
    <inherits name="com.smartclient.theme.simplicity.SimplicityResources"/>

    <inherits name='com.orange.links.Links' />
    <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />

    <set-property name="compiler.emulatedStack" value="true" />
    <set-configuration-property name="compiler.emulatedStack.recordLineNumbers"
        value="true" />
    <set-configuration-property name="compiler.emulatedStack.recordFileNames"
        value="true" />
    <inherits name='com.google.gwt.widgetideas.WidgetIdeas' />
    <inherits name='com.google.gwt.libideas.LibIdeas' />


  <!--  Specify the app entry point class.-->                        
  <entry-point class='dei.vlab.communication.client.entrypoint.Application'/>

  <!--  Specify the paths for translatable code    -->              
  <source path='client'/>
  <source path='shared'/>

</module>